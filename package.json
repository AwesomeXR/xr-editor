{
  "name": "xr-editor",
  "version": "1.1.0",
  "files": [
    "dist",
    "esm"
  ],
  "main": "esm/index.js",
  "module": "esm/index.js",
  "scripts": {
    "ci": "prettier --check src && tsc -p tsconfig.json --noEmit",
    "release": "npm run ci && npm run export:build && npm run export:deploy && release-it",
    "gen:theme": "node ./gen-theme.js",
    "app:dev": "MODE=development webpack server -c webpack.app.config.js --stats-children",
    "export:emitType": "tsc -p tsconfig.build.json --emitDeclarationOnly --outDir esm",
    "export:build": "rm -rf dist/* esm/* && MODE=production webpack build -c webpack.export.config.js && npm run export:emitType",
    "export:deploy": "ah-web-cli deploy --publicDir=dist --stableAssetExts=js,css,dds,env,png --chunkSize=1"
  },
  "devDependencies": {
